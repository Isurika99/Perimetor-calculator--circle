<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Myapplication01"
        tools:targetApi="31">
        <activity
            android:name=".circle"
            android:exported="false"
            android:label="@string/title_activity_circle"
            android:theme="@style/Theme.Myapplication01" />
        <activity
            android:name=".triangle"
            android:exported="false"
            android:label="@string/title_activity_triangle"
            android:theme="@style/Theme.Myapplication01" />
        <activity
            android:name=".trapezoid"
            android:exported="false"
            android:label="@string/title_activity_trapezoid"
            android:theme="@style/Theme.Myapplication01" />
        <activity
            android:name=".rhombus"
            android:exported="false"
            android:label="@string/title_activity_rhombus"
            android:theme="@style/Theme.Myapplication01" />
        <activity
            android:name=".parallelogram"
            android:exported="false"
            android:label="@string/title_activity_parallelogram"
            android:theme="@style/Theme.Myapplication01" />
        <activity
            android:name=".rectangle"
            android:exported="false"
            android:label="@string/title_activity_rectangle"
            android:theme="@style/Theme.Myapplication01" />
        <activity
            android:name=".square"
            android:exported="false"
            android:label="@string/title_activity_square"
            android:theme="@style/Theme.Myapplication01" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>